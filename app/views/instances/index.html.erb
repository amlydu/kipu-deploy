<div class="row">
  <% @instances.each do |instance| %>
    <div id="<%= "instance_#{instance.id}" %>" class="col-lg-4 col-md-6">
      <%= render partial: "instance_box", locals: { instance: instance } %>
    </div>
  <% end %>
</div>
